default vesamenu.c32
prompt 0
timeout 50

menu title PXE Install Server
#menu background background.jpg

label harddisk
  menu label Local Hard Disk
  localboot 0

label install-osd-node
  menu label OSD Node
  kernel sle12sp3/linux
  append load ramdisk=1 initrd=sle12sp3/initrd netsetup=dhcp install=http://admin.ses-demo.suse.ru/install/SLE12SP3 autoyast=http://admin.ses-demo.suse.ru/install/autoinst.xml


